<%- id = "koi-#{ new_uuid }" -%>
<%= f.association attr.to_sym,
  collection: attr.to_s.classify.constantize.all,
  include_blank: true,
  wrapper_html: { class: "form--medium" }, 
  input_html: { id: id, data: { tag_input: "" } } %>

<%= render "koi/admin_crud/form_field_collection_errors", f: f, attr: attr %>

