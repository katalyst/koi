<%- group_settings = settings.group_by(&:group) -%>

<%- group_settings.each do |group, settings| -%>
  <%- group = "Remaining" if group.blank? -%>
  <%= render 'koi/settings/settings', group: group, settings: settings %>
<%- end -%>

<%- if current_admin.god? -%>
  <%= render 'koi/settings/extra' %>
<%- end -%>
