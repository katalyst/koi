<% if resource.errors.any? -%>
  $("#modal-for-setting").html("<%= escape_javascript render("modal_setting") %>");
<% else %>
  $("#modal-for-setting").modal('hide');
  $("#settings-list").html("<%= escape_javascript render("/koi/settings/settings_list",
                                        { settings: Setting.where(prefix: resource.prefix) }) %>");
<% end %>
