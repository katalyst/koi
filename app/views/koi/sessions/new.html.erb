<%= render "layouts/koi/navigation_header" %>
<%= form_with(model: admin_user, url: admin_session_path) do |f| %>
  <%= f.govuk_fieldset legend: nil do %>
    <%= f.govuk_email_field :email, autofocus: true %>
    <%= f.govuk_password_field :password %>
  <% end %>
  <%= f.govuk_submit "Log in", class: "button" %>
<% end %>
