<% content_for :title, "New navigation" %>
<% content_for(:breadcrumbs) { link_to "Navigations", katalyst_navigation.menus_path } %>
<% content_for :form_class, "govuk-forms" %>

<%= form_with model: menu, url: katalyst_navigation.menus_path, builder: GOVUKDesignSystemFormBuilder::FormBuilder do |form| %>
  <%= form.govuk_text_field :title %>
  <%= form.govuk_text_field :slug %>
  <%= form.govuk_submit %>
<% end %>
