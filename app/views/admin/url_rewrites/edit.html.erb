<% content_for :title, "Edit URL rewrite" %>
<% content_for(:breadcrumbs) do %>
  <%= link_to "URL rewrites", admin_url_rewrites_path %> >
  <%= link_to url_rewrite.from.delete_prefix("/"), admin_url_rewrite_path(url_rewrite) %>
<% end %>

<%= form_with(model: url_rewrite, url: admin_url_rewrite_path(url_rewrite)) do |form| %>
  <%= render "form_fields", form: %>

  <div class="actions">
    <%= form.admin_save %>
    <%= form.admin_delete %>
  </div>
<% end %>
