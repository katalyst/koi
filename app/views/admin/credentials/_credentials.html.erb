<%# locals: (admin_user:) %>

<%= table_with(id: dom_id(admin_user, :credentials), collection: admin_user.credentials) do |t, c| %>
  <% t.text(:nickname, label: "Name") %>
  <% t.date(:updated_at, label: "Last use") do |date| %>
    <%= date unless c.created_at == c.updated_at %>
  <% end %>
<% end %>
