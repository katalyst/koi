<% content_for :html do %>
  <body style="width:950px;overflow:hidden;">

    <div class="row h-400">
      <header class="titlebar span24">
        <%= render "title_bar" %>
      </header>

      <nav class="bg-mediumgrey pad-t-2 pad-b-1 span24">
        <div class="space-l-2">
          <%= render "tag_bar" %>
        </div>
      </nav>
    </div>

    <div class="row">
      <div class="span24 bg-lightgrey">
        
        <div class="row">
          <div class="span18 h-600" style="padding-right:10px;overflow:auto;">
            <nav>
              <%= render "tool_bar" %>
            </nav>
            <div class=" pad-1">
              <%= render "asset_table" %>
            </div>
          </div>
          <div class="asset-manager-bg-darkgrey span6 h-600" style="margin-left:0;">
            <%= content_for?(:assets) ? yield(:assets) : yield %>
          </div>
        </div>
      </div>
    </div>

  </body>
<% end %>

<%= render :file => 'layouts/koi/html' %>

