  <li><%= link_to "Sitemap", koi_engine.sitemap_nav_items_path %></li>
  <li class="dropdown">
    <a href="#" class="dropdown-toggle" data-toggle="dropdown">Module <b class="caret"></b> </a>
    <ul class="dropdown-menu">
      <% Koi::Menu.items.each do |key, value| %>
        <%= content_tag :li, link_to(raw(key), value) %>
      <% end %>
    </ul>
  </li>
  <li class="dropdown">
    <a href="#" class="dropdown-toggle" data-toggle="dropdown">Assets <b class="caret"></b> </a>
