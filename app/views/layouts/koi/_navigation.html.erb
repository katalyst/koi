    <a href="#" class="dropdown-toggle" data-toggle="dropdown">Advanced <b class="caret"></b> </a>
    <ul class="dropdown-menu">
      <%- unless Koi::Menu.advanced.blank? -%>
        <% Koi::Menu.advanced.each do |key, value| %>
